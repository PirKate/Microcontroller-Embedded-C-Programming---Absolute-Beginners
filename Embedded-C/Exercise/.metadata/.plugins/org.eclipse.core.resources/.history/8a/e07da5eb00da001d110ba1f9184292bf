#include <stdio.h>

int main (void) {

	printf("enter 6 characters of your choice: ");
	fflush(stdout);

	char q[6];
	int cnt = 0;
	printf("ASCII codes: ");
	while (cnt < 6) {
		scanf("%c", &q);
	}

	printf("press enter to exit the app \n");

	while(getchar() != '\n'){

	}
	getchar();

}
